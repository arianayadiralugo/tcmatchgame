{
    "include": [
        "./src/**/*",
        "./src/**/*.jsx",
        "./src/**/*.js",
    ],
    "compilerOptions": {
        "allowJs": true,
    "allowSyntheticDefaultImports": true,
        "strict": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
        "lib": [
            "dom",
            "es2015"
        ],
        "jsx": "react-jsx"
    }
}